{
    "sqltools.connections": [
        {
            "mysqlOptions": {
                "authProtocol": "xprotocol"
            },
            "previewLimit": 50,
            "server": "127.0.0.1",
            "port": 3306,
            "driver": "MySQL",
            "name": "mystorybook-dev (Docker)",
            "group": "mystorybook",
            "database": "mystorybook",
            "username": "app",
            "password": "app",
            "connectString": "app:app@127.0.0.1:3306/mystorybook",
            "connectionTimeout": 5
        }
    ],
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "jest.jestCommandLine": "yarn test",
    "jest.autoRun": "off",
    "jestrunner.jestCommand": "yarn run test --"
}
